<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hunt.dao.SysUserPermissionMapper">
    <resultMap id="baseResultMap" type="com.hunt.model.entity.SysUserPermission">
        <result column="id" property="id" jdbcType="BIGINT"/>
        <result column="sys_user_id" property="sysUserId" jdbcType="BIGINT"/>
        <result column="sys_permission_id" property="sysPermissionId" jdbcType="BIGINT"/>
        <result column="is_final" property="isFinal" jdbcType="INTEGER"/>
        <result column="rank" property="rank" jdbcType="BIGINT"/>
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
        <result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>
        <result column="create_by" property="createBy" jdbcType="BIGINT"/>
        <result column="update_by" property="updateBy" jdbcType="BIGINT"/>
        <result column="state" property="state" jdbcType="INTEGER"/>
    </resultMap>
    <!--  -->
    <sql id="sql_column">
        sup.id id ,
        sup.sys_user_id sysUserId ,
        sup.sys_permission_id sysPermissionId ,
        sup.is_final isFinal ,
        sup.rank rank ,
        sup.create_time createTime ,
        sup.update_time updateTime ,
        sup.create_by createBy ,
        sup.update_by updateBy ,
        sup.state state
    </sql>

    <sql id="sql_column_where">
        <if test="id !=null ">
            AND sup.id = #{id}
        </if>
        <if test="sysUserId !=null ">
            AND sup.sys_user_id = #{sysUserId}
        </if>
        <if test="sysPermissionId !=null ">
            AND sup.sys_permission_id = #{sysPermissionId}
        </if>
        <if test="isFinal !=null ">
            AND sup.is_final = #{isFinal}
        </if>
        <if test="rank !=null ">
            AND sup.rank = #{rank}
        </if>
        <if test="createTime !=null ">
            AND sup.create_time = #{createTime}
        </if>
        <if test="updateTime !=null ">
            AND sup.update_time = #{updateTime}
        </if>
        <if test="createBy !=null ">
            AND sup.create_by = #{createBy}
        </if>
        <if test="updateBy !=null ">
            AND sup.update_by = #{updateBy}
        </if>
        <if test="state !=null ">
            AND sup.state = #{state}
        </if>
    </sql>

    <insert id="insertSysUserPermission" parameterType="SysUserPermission" useGeneratedKeys="true" keyProperty="id">
        insert into sys_user_permission sup
        <trim prefix="(" suffix=")" suffixOverrides=",">
            sup.id,
            sup.sys_user_id,
            sup.sys_permission_id,
            sup.is_final,
            sup.rank,
            sup.create_time,
            sup.update_time,
            sup.create_by,
            sup.update_by,
            sup.state,
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id !=null">
                sup.id = #{id},
            </if>
            <if test="sysUserId !=null">
                sup.sys_user_id = #{sysUserId},
            </if>
            <if test="sysPermissionId !=null">
                sup.sys_permission_id = #{sysPermissionId},
            </if>
            <if test="isFinal !=null">
                sup.is_final = #{isFinal},
            </if>
            <if test="rank !=null">
                sup.rank = #{rank},
            </if>
            <if test="createTime !=null">
                sup.create_time = #{createTime},
            </if>
            <if test="updateTime !=null">
                sup.update_time = #{updateTime},
            </if>
            <if test="createBy !=null">
                sup.create_by = #{createBy},
            </if>
            <if test="updateBy !=null">
                sup.update_by = #{updateBy},
            </if>
            <if test="state !=null">
                sup.state = #{state},
            </if>
        </trim>
    </insert>


    <update id="updateSysUserPermission" parameterType="SysUserPermission">
        update sys_user_permission sup
        <set>
            <if test="id !=null">
                sup.id = #{id},
            </if>
            <if test="sysUserId !=null">
                sup.sys_user_id = #{sysUserId},
            </if>
            <if test="sysPermissionId !=null">
                sup.sys_permission_id = #{sysPermissionId},
            </if>
            <if test="isFinal !=null">
                sup.is_final = #{isFinal},
            </if>
            <if test="rank !=null">
                sup.rank = #{rank},
            </if>
            <if test="createTime !=null">
                sup.create_time = #{createTime},
            </if>
            <if test="updateTime !=null">
                sup.update_time = #{updateTime},
            </if>
            <if test="createBy !=null">
                sup.create_by = #{createBy},
            </if>
            <if test="updateBy !=null">
                sup.update_by = #{updateBy},
            </if>
            <if test="state !=null">
                sup.state = #{state},
            </if>
        </set>
        where sup.id =#{id} and state=1
    </update>

    <select id="selectSysUserPermission" parameterType="SysUserPermission" resultType="SysUserPermission">
        select * from sys_user_permission sup where
        <where>
            <include refid="sql_column_where"/>
        </where>
    </select>

    <select id="selectSysUserPermissionById" parameterType="Long" resultType="SysUserPermission">
        select * from sys_user_permission sup where sup.id =#{id} and $meta.aliasName.state=1
    </select>

    <select id="selectSysUserPermissionAll" resultType="SysUserPermission">
        select * from sys_user_permission where state=1
    </select>

</mapper>